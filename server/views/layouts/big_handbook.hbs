<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link type="image/svg+xml" sizes="16x16" rel="icon" href="./images/favicon-16x16.png?v=1.16">
        <link type="image/svg+xml" sizes="32x32" rel="icon" href="./images/favicon-32x32.png?v=1.16">

        {{#each style_stack}}
            <link rel="stylesheet" href="{{this}}">
        {{/each}}

        <title>{{alias}} | CRM DEMO</title>
    </head>

    <body>
        {{> preloader}}
        {{> header}}
        <main>
            <div class='handbook'>
                {{{body}}}
            </div>
            <div id="hb_edit_form"></div>
            <div id="dialog_box"></div>
        </main>
        {{> footer}}
        <div style='display: none'>
            {{#each script_stack}}
                <script type="text/javascript" src="{{this}}"></script>
            {{/each}}
        </div>
    </body>
</html>
